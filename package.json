  "description": "Full Stack Website for Union Realty Group LLC.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "tsc -p src/client && webpack --mode production",
    "start": "yarn && npm run build && tsc --skipLibCheck -p src/server && nodemon src/server/dist/index.js && node src/server/dist/index.js > union_log.log 2> union_error.log",
